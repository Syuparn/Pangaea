assertEq(
  %{'a: 1, 'b: 2, 'c: 3, 'd: 4}.flipflop({|k, v| v > 1}, {|k, v| v > 3}),
  [['b, 2], ['c, 3], ['d, 4]],
)
# not found
assertEq(
  %{'a: 1}.flipflop({|k, v| v > 1}, {|k, v| v < 4}),
  [],
)
# multiple
assertEq(
  %{'a: 1, 'b: 2, 'c: 3, 'd: 4, 'e: 5, 'f: 6}.flipflop({|k, v| v % 3 == 1}, {|k, v| v % 3 == 2}),
  [['a, 1], ['b, 2], ['d, 4], ['e, 5]],
)
# only start found
assertEq(
  %{'a: 1, 'b: 2, 'c: 3, 'd: 4}.flipflop({|k, v| v > 1}, {|k, v| v > 6}),
  [['b, 2], ['c, 3], ['d, 4]],
)
